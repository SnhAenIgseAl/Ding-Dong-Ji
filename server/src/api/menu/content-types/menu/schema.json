{
  "kind": "collectionType",
  "collectionName": "menus",
  "info": {
    "singularName": "menu",
    "pluralName": "menus",
    "displayName": "Menu"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "menu_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "menu_price": {
      "type": "decimal",
      "required": true
    },
    "menu_desc": {
      "type": "text"
    },
    "need_vip": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "menu_img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "menu_flavors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::menu-flavor.menu-flavor",
      "inversedBy": "menus"
    },
    "menu_number": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "menu_class": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu-class.menu-class",
      "inversedBy": "menus"
    },
    "stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store",
      "inversedBy": "menus"
    }
  }
}
