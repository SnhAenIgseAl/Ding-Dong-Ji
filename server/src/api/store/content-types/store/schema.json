{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store_name": {
      "type": "string",
      "minLength": 1,
      "required": true,
      "unique": true
    },
    "store_address": {
      "type": "string",
      "minLength": 1,
      "required": true,
      "unique": true
    },
    "store_opening": {
      "type": "time",
      "required": true
    },
    "store_closing": {
      "type": "time",
      "required": true
    },
    "longitude": {
      "type": "decimal"
    },
    "latitude": {
      "type": "decimal"
    },
    "order_lists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-list.order-list",
      "mappedBy": "store"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "store"
    },
    "menus": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::menu.menu",
      "mappedBy": "stores"
    }
  }
}
