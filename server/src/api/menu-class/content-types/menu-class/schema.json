{
  "kind": "collectionType",
  "collectionName": "menu_classes",
  "info": {
    "singularName": "menu-class",
    "pluralName": "menu-classes",
    "displayName": "MenuClass"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "class_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "class_icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "menus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu.menu",
      "mappedBy": "menu_class"
    }
  }
}
