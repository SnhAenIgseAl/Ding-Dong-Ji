{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "avatar": {
      "type": "string"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "user"
    },
    "order_lists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-list.order-list",
      "mappedBy": "user"
    },
    "comment_likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment-like.comment-like",
      "mappedBy": "user"
    },
    "discounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::discount.discount",
      "mappedBy": "user"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "user"
    },
    "vip": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vip.vip",
      "mappedBy": "user"
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store",
      "inversedBy": "user"
    }
  }
}
